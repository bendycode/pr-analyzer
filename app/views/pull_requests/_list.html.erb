<h3><%= category.titleize %> PRs</h3>
<table>
  <thead>
    <tr>
      <th>Number</th>
      <th>Title</th>
      <th>State</th>
      <th>Created At</th>
      <th>Updated At</th>
    </tr>
  </thead>
  <tbody>
    <% prs.each do |pr| %>
      <tr>
        <td><%= pr.number %></td>
        <td><%= pr.title %></td>
        <td><%= pr.state %></td>
        <td><%= pr.gh_created_at %></td>
        <td><%= pr.gh_updated_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
